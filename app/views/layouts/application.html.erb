<!DOCTYPE html>
<html>
  <head>
    <title>Cloudchart</title>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= csrf_meta_tags %>
  </head>

  <body>
    <header>
      <%= render 'shared/header' %>
    </header>

    <main>
      <%= render 'shared/flash' %>
      <%= yield %>
    </main>

    <footer>
      <%= render 'shared/footer' %>
    </footer>

    <%= javascript_include_tag 'application' %>
  </body>

</html>
